{
  "name": "@nib-components/react-product-carousel",
  "version": "1.1.0-preview.3",
  "description": "React product carousel.",
  "dependencies": {
    "@nib-styles/colors": "^1.1.1",
    "@nib-styles/sass-grid": "^1.0.0",
    "@nib-styles/sass-spacing": "^1.0.0",
    "@nib-styles/v2-icons": "^2.6.2",
    "@nib-styles/v2-typography": "^4.0.2",
    "classnames": "^2.2.5",
    "nuka-carousel": "git+https://github.com/lauriejones/nuka-carousel"
  },
  "devDependencies": {
    "@nib-components/react-product-header": "^0.1.0",
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-incremental": "^3.0.1",
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-testutils-query": "^0.1.1",
    "react-testutils-render": "^0.1.1",
    "readdir": "^0.0.13",
    "sass-composer": "2.0.0-beta12"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "scripts": {
    "build": "babel ./index.js ./lib/**/*.js -d ./dist --source-maps",
    "example.css.build": "sass-composer index.scss -o example/index.bundled.css",
    "example.js.build": "browserify -v --debug -t babelify example/index.js -o example/index.bundled.js",
    "example.build": "npm run build && npm run example.js.build && npm run example.css.build",
    "prepublish": "npm run example.build"
  },
  "directories": {
    "example": "example"
  },
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nib-components/react-product-carousel.git"
  },
  "bugs": {
    "url": "https://github.com/nib-components/react-product-carousel/issues"
  },
  "homepage": "https://github.com/nib-components/react-product-carousel#readme"
}
